<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
    <context>
        <name>Vulnerable App Context</name>
        <desc>Security testing context for vulnerable application</desc>
        <inscope>true</inscope>
        <incregexes>http://localhost:8080/.*</incregexes>
        <tech>
            <include>Db</include>
            <include>Db.PostgreSQL</include>
            <include>Db.MySQL</include>
            <include>Db.H2</include>
            <include>Language</include>
            <include>Language.Java</include>
            <include>Language.Java.Spring</include>
            <include>OS</include>
            <include>OS.Linux</include>
            <include>SCM</include>
            <include>SCM.Git</include>
            <include>WS</include>
        </tech>
        <urlparser>
            <class>org.zaproxy.zap.model.StandardParameterParser</class>
            <config>{"kvps":"&amp;","kvs":"=","struct":[]}</config>
        </urlparser>
        <postparser>
            <class>org.zaproxy.zap.model.StandardParameterParser</class>
            <config>{"kvps":"&amp;","kvs":"=","struct":[]}</config>
        </postparser>
        <authentication>
            <type>1</type>
            <strategy>EACH_RESP</strategy>
            <pollurl></pollurl>
            <polldata></polldata>
            <pollheaders></pollheaders>
            <pollfreq>60</pollfreq>
            <pollunits>REQUESTS</pollunits>
            <form>
                <loginurl>http://localhost:8080</loginurl>
                <loginbody></loginbody>
                <loginpageurl></loginpageurl>
            </form>
            <httpauth>
                <host>localhost</host>
                <realm></realm>
                <port>8080</port>
            </httpauth>
            <script>
                <name></name>
                <engine></engine>
                <script></script>
            </script>
        </authentication>
        <users>
            <user>1;true;alice;alice:alice123</user>
            <user>2;false;bob;bob:bob123</user>
            <user>3;false;admin;admin:admin123</user>
        </users>
        <forceduser>1</forceduser>
        <session>
            <type>0</type>
        </session>
        <authorization>
            <type>0</type>
            <basic>
                <header></header>
                <body></body>
                <logic>AND</logic>
                <code>-1</code>
            </basic>
        </authorization>
    </context>
</configuration>
